{
  "name": "payone/shopware",
  "type": "shopware-platform-plugin",
  "description": "PAYONE Payment Plugin",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "PAYONE GmbH",
      "email": "feedback@bspayone.com",
      "homepage": "https://www.bspayone.com"
    },
    {
      "name": "Kellerkinder GmbH",
      "email": "hallo@kellerkinder.de",
      "homepage": "https://www.kellerkinder.de/",
      "role": "Contributor"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "dms/phpunit-arraysubset-asserts": "^0.1.0",
    "fzaninotto/faker": "1.8.0",
    "k10r/codestyle": "^1.0",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpunit/phpunit": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "PayonePayment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PayonePayment\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "shopware-plugin-class": "PayonePayment\\PayonePayment",
    "plugin-icon": "src/Resources/public/plugin.png",
    "copyright": "(c) by PAYONE GmbH",
    "label": {
      "de-DE": "PAYONE Payment",
      "en-GB": "PAYONE Payment"
    },
    "description": {
      "de-DE": "PAYONE Payment Plugin",
      "en-GB": "PAYONE Payment Plugin"
    },
    "manufacturerLink": {
      "de-DE": "https://www.bspayone.com",
      "en-GB": "https://www.bspayone.com"
    },
    "supportLink": {
      "de-DE": "https://www.bspayone.com",
      "en-GB": "https://www.bspayone.com"
    }
  }
}
