            <tag name="shopware.payment.method.async" />
        </service>

        <service id="PayonePayment\PaymentHandler\PayoneWeChatPayPaymentHandler"
                 parent="PayonePayment\PaymentHandler\AbstractAsynchronousPayonePaymentHandler">

