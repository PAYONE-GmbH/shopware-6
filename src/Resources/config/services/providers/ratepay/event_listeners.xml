<?xml version="1.0" encoding="UTF-8" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="PayonePayment\Provider\Ratepay\EventListener\CheckoutConfirmRatepayEventListener">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="PayonePayment\Provider\Ratepay\Service\InstallmentService" />
            <argument type="service" id="PayonePayment\Provider\Ratepay\Service\ProfileService" />

            <tag name="kernel.event_subscriber" />
        </service>

        <service id="PayonePayment\Provider\Ratepay\EventListener\KernelEventListener">
            <argument type="service" id="PayonePayment\Provider\Ratepay\Service\ProfileService" />
            <argument type="service" id="PayonePayment\Components\ConfigReader\ConfigReader" />
            <argument type="service" id="PayonePayment\Provider\Ratepay\PaymentHandler\DebitPaymentHandler" />
            <argument type="service" id="payone.request_enricher_chain.profile.ratepay.debit" />
            <argument type="service" id="PayonePayment\Provider\Ratepay\PaymentHandler\InstallmentPaymentHandler" />
            <argument type="service" id="payone.request_enricher_chain.profile.ratepay.installment" />
            <argument type="service" id="PayonePayment\Provider\Ratepay\PaymentHandler\InvoicePaymentHandler" />
            <argument type="service" id="payone.request_enricher_chain.profile.ratepay.invoice" />

            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
