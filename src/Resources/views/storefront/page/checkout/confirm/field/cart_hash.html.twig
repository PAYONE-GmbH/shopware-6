{% block page_checkout_confirm_payone_carthash_field %}
    {% set value = null %}

    {% if page.extensions.payone and page.extensions.payone.cartHash %}
        {% set value = page.extensions.payone.cartHash %}
    {% endif %}

    <input type="hidden" form="confirmOrderForm" id="payoneCartHash" name="carthash" value="{{ value }}"/>
{% endblock %}