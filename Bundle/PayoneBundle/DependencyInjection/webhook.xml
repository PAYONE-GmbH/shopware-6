<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="true"/>

        <service id="PayonePayment\Bundle\PayoneBundle\Webhook\Factory\WebhookHandlerFactory">
            <argument type="service" id="service_container"/>
        </service>

        <service id="PayonePayment\Bundle\PayoneBundle\Webhook\Processor\WebhookProcessor">
            <argument type="service" id="PayonePayment\Bundle\PayoneBundle\Webhook\Factory\WebhookHandlerFactory"/>
        </service>

        <service id="PayonePayment\Bundle\PayoneBundle\Webhook\Handler\TransactionStatusWebhookHandler" />
    </services>
</container>
